{
	"plugins": ["/usr/lib/node_modules/@ckeditor/jsdoc-plugins/lib/export-fixer/export-fixer.js"],
	"recurseDepth": 10,
	"source": {
		"include": ["./"],
		"exclude": ["node_modules"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc","closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false
	},
	"opts": {
		"destination": "docs",
		"recurse": true,
		"readme": "README.md"
	}
}